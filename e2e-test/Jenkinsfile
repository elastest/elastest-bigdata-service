def elastest_url = ''
node('et_in_et'){
    elastest(tss: ['EBS'], surefireReportsPattern: '**/target/surefire-reports/TEST-*.xml', project: 'ETinET', sut: 11) {        
        stage ('docker container')
            def mycontainer = docker.image('elastest/ebs-spark:latest')
            mycontainer.pull()
            mycontainer.inside()  {
                
                stage ('prepare test')
                    git 'https://github.com/elastest/demo-projects.git'
                    elastest_url = env.ET_SUT_PROTOCOL + '://' + env.ET_SUT_HOST + ':' + env.ET_SUT_PORT
                sh "env | sort"    
                stage ("Run tests")
                    try {
                        sh "cd ebs-test;run.sh"
                    } catch(e) {
                        echo 'Err: ' + e.toString()
                    } 
            }
    }
}
